{
    "workbench.colorTheme": "Tokyo City",
    "editor.fontFamily": "Comic Code, monospace",
    "editor.letterSpacing": 0.02,
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "top",
    "workbench.activityBar.orientation": "vertical",
    "workbench.settings.applyToAllProfiles": [],
    "editor.fontSize": 15,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "JetBrains Mono, monospace",
    "explorer.compactFolders": false,
    "workbench.startupEditor": "none",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "css.lint.vendorPrefix": "ignore",
    "css.lint.unknownProperties": "ignore",
    "workbench.editor.customLabels.patterns": {
        "**/index.*": "${dirname}/${filename}.${extname}",
        "**/?id?.*": "${dirname}/${filename}.${extname}",
        "**/page.tsx": "${dirname}/${filename}.${extname}",
        "**/new.tsx": "${dirname}/${filename}.${extname}",
        "**/route.ts": "${dirname}/${filename}.${extname}"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.openRepositoryInParentFolders": "never",
    "liveServer.settings.donotShowInfoMsg": true,
    "svelte.enable-ts-plugin": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "update.showReleaseNotes": false,
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.env.example": true,
        "local.env": true,
        "**/.next": true,
        "**/dist": true,
        "**/.vscode": true,
        "**/.gitignore": true,
        "**/next.config.mjs": true,
        "**/next-env.d.ts": true,
        "**/postcss.config.mjs": true,
        "**/node_modules": true,
        "**/package-lock.json": true,
        ".husky": true,
        ".github": true,
        ".dockerignore": true,
        ".prettierignore": true,
        ".prettierrc": true
    },
    "workbench.preferredDarkColorTheme": "Tokyo Night",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Tokyo (Light)",
    "editor.lineHeight": 1.85,
    "workbench.tree.indent": 14,
    "workbench.layoutControl.enabled": false,
    "liveServer.settings.donotVerifyTags": true,
    "vscode_custom_css.imports": [
        "file:///styles/vscode/code-styles.min.css"
    ],
    "editor.accessibilitySupport": "off",
    "go.toolsManagement.autoUpdate": true,
    "nodejs-testing.extensions": [
        {
            "extensions": [
                "mjs",
                "cjs",
                "js"
            ],
            "parameters": []
        },
        {
            "extensions": [
                "mts",
                "cts",
                "ts"
            ],
            "parameters": [
                "--import",
                "tsx"
            ]
        }
    ],
    "chat.commandCenter.enabled": false,
    "continue.enableTabAutocomplete": false,
    "makefile.configureOnOpen": false,
    "continue.telemetryEnabled": false,
    "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
    "typescript.tsserver.maxTsServerMemory": 4096,
    "typescript.tsdk": "6000",
    "diffEditor.ignoreTrimWhitespace": false
}